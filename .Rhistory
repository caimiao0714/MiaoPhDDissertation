plp = function(t, beta = 2, theta = 10) return(beta*theta^(-beta)*t^(beta-1))
sim_plp_tau = function(tau = 30,
beta = 2,
theta = 10){
# initialization
s = 0; t = 0
while (max(t) <= tau) {
u <- runif(1)
s <- s - log(u)
t_new <- theta*s^(1/beta)
t <- c(t, t_new)
}
t = t[c(-1, -length(t))]
return(t)
}
